{
  "name": "@mono-app/mini",
  "version": "1.0.0",
  "description": "使用Taro创建的小程序项目",
  "main": "index.js",
  "author": "chenjie@thundersdata.com",
  "private": true,
  "scripts": {
    "tsc": "tsc -p ./tsconfig.json",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@mono-app/api": "*",
    "@mono-app/business": "*",
    "@mono-app/utils": "*",
    "@mono-app/platform": "*",
    "@tarojs/components": "3.4.10",
    "@tarojs/plugin-framework-react": "3.4.10",
    "@tarojs/react": "3.4.10",
    "@tarojs/runtime": "3.4.10",
    "@tarojs/taro": "3.4.10",
    "@tarojs/taro-weapp": "^2.2.10",
    "lodash-es": "^4.17.21",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "^3.4.10",
    "@tarojs/mini-runner": "3.4.10",
    "@tarojs/plugin-babel": "^2.2.10",
    "@tarojs/webpack-runner": "3.4.10",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "3.4.10",
    "stylelint": "^14.4.0"
  }
}